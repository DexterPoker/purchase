      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT PO_HEADERS_ALL.po_header_id, PO_HEADERS_ALL.po_number, 
      PO_HEADERS_ALL.po_type, PO_HEADERS_ALL.vender_code, 
      PO_HEADERS_ALL.status, PO_LINES_ALL.po_line_id, 
      PO_LINES_ALL.po_header_id AS Expr1, PO_LINES_ALL.item_num, 
      PO_LINES_ALL.line_num, PO_LINES_ALL.quantity, PO_LINES_ALL.received_quantity, 
      PO_LINES_ALL.price
FROM PO_HEADERS_ALL INNER JOIN
      PO_LINES_ALL ON 
      PO_HEADERS_ALL.po_header_id = PO_LINES_ALL.po_header_id
WHERE (PO_HEADERS_ALL.po_number = ?) AND (PO_HEADERS_ALL.po_type = 'plan')</CommandText>
        <DataSourceName>ConnectionString</DataSourceName>
      </Query>
